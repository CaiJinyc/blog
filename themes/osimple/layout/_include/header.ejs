<header>
  <div class="home-title-content">
    <h1 class="title">
        <%= title || config.title %>
    </h1>
    <p class="desc">
        <%= config.description %>
      </p>
  </div>
  <div class="menu-inner">
      <% Object.keys(theme.menu).forEach(function(key) { %>
        <a href="<%= url_for(theme.menu[key]) %>"><%= key %></a>
      <% }); %>
  </div>
</header>